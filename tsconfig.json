{
  "compilerOptions": {
    "target": "es5",
    "lib":[
      "ES2020"
    ],
    "module": "commonjs",
    "outDir": "dist",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/**/*.ts",
    "global.d.ts"
  ]
}
